<?xml version="1.0" encoding="utf-8"?>
<resources xmlns:xliff="urn:oasis:names:tc:xliff:document:1.2">

    <!-- Title for the brailleback, preferences activity. -->
    <string name="brailleback_setup_title" translatable="false">BrailleBack</string>

    <!-- Displayed on the accessibility settings screen when the user has selected BrailleBack. Note that in the phrase "the letter l", the letter is a lowercase L, not an uppercase I. -->
    <string name="service_description">When BrailleBack is on, your device will connect to a paired bluetooth braille display and present the screen content in braille. Keys on the braille display can be used to navigate the screen, activate items and enter text. For more information, press space and dots 1, 2 and 3 (the letter l in braille) on your display when connected.</string>

    <!-- Displayed as subtext on the accessibility service preference in Settings. -->
    <string name="service_summary">Provide braille output</string>

    <!-- Name of the input method for typing using the braille keys on the display. -->
    <string name="braille_ime_name">Braille hardware keyboard</string>

    <!-- Title for the wizard that guides the user through the process of setting up the braille hardware keyboard. -->
    <string name="ime_setup_wizard_name">Braille Hardware Keyboard Wizard</string>
    <!-- Title for the step of the braille hardware keyboard wizard that lets the user know to enable the input method. -->
    <string name="ime_setup_need_enable_title">Enable braille hardware keyboard</string>
    <!-- Dialog message that lets the user know to enable the input method -->
    <string name="ime_setup_need_enable_message">To use the braille hardware keyboard, you need to enable it in the input method settings and then set it as the default input method.</string>
    <!-- Label for a button in the braille input method wizard that lets the user skip the setup. [CHAR_LIMIT=15] -->
    <string name="ime_setup_skip">Skip</string>
    <!-- Button in the enable braille hardware keyboard input method wizard that opens the system dialog to enable the ime. [CHAR_LIMIT=45] -->
    <string name="ime_setup_need_enable_positive">Open Input Method Settings</string>
    <!-- Title for the dialog that lets the user know to switch to the braille hardware keyboard input method. -->
    <string name="ime_setup_need_default_title">Set Default Input Method</string>
    <!-- Text for the dialog that lets the user know to switch to the braille hardware keyboard input method. -->
    <string name="ime_setup_need_default_message">To use the braille hardware keyboard, you need to set it as the default input method.</string>
    <!-- Label for a button in the dialog that lets the user know to switch to the braille hardware keyboard input method. Opens the system dialog that lets the user switch to an input method. [CHAR_LIMIT=60] -->
    <string name="ime_setup_need_default_positive">Open Input Method Picker</string>


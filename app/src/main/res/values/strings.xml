    <string name="permlab_displayConnect">Connect to a braille display</string>
    <!-- Permission description. -->
    <string name="permdesc_displayConnect">Allows an app to write output to and and read keyboad input from a braille display</string>

    <string name="checkmark_not_checked" translatable="false">( )</string>
    <string name="checkmark_checked" translatable="false">(x)</string>

